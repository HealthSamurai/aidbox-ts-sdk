{
	"name": "@health-samurai/aidbox-client",
	"version": "0.0.0-alpha.2",
	"description": "A client library for communicating with Aidbox",
	"author": "Health Samurai",
	"type": "module",
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.types.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": "./dist/src/index.js"
	},
	"scripts": {
		"build": "rm -rf dist && mkdir -p dist && swc src -d dist -D && tsc -b --force && echo success",
		"format": "biome format",
		"format:check": "biome format",
		"format:fix": "biome format --write",
		"lint": "biome check",
		"lint:check": "biome check",
		"lint:fix": "biome check --write",
		"tsc:check": "tsc -b --noEmit",
		"typecheck": "tsc -b --noEmit",
		"generate-types": "tsx scripts/generate-types.ts",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"doc": "typedoc"
	},
	"dependencies": {
		"@types/json-patch": "^0.0.33",
		"yaml": "^2.8.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/HealthSamurai/aidbox-ts-sdk"
	},
	"devDependencies": {
		"@atomic-ehr/codegen": "^0.0.3",
		"@types/node": "^24.2.0",
		"tsx": "^4.20.4",
		"typedoc": "^0.28.14",
		"typescript": "~5.8.3",
		"vitest": "^3.2.4"
	}
}
